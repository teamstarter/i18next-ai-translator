{
  "name": "@teamstarter/i18next-ai-translator",
  "version": "1.0.0",
  "description": "Fill un-translated keys of i18next language bundles using reference knowledge files.",
  "main": "dist/index.js",
  "types": "dist/types/*",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "bin/**/*.js"
  ],
  "bin": {
    "i18next-ai-translator": "./bin/i18next-ai-translator.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf dist/*",
    "dev": "ts-node ./src/index.ts",
    "start": "node ./bin/i18next-ai-translator.js",
    "compile": "npm run clean && tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint --ext .js,.ts",
    "lint:fix": "eslint --ext .js,.ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teamstarter/i18next-ai-translator.git"
  },
  "keywords": [
    "i18next",
    "translation",
    "ai",
    "chatgpt",
    "localization",
    "internationalization"
  ],
  "author": {
    "name": "Bl√©za Plegnon",
    "email": "bleza.plg@gmail.com"
  },
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/teamstarter/i18next-ai-translator/issues"
  },
  "homepage": "https://github.com/teamstarter/i18next-ai-translator#readme",
  "dependencies": {
    "debug": "^4.4.3",
    "dotenv": "^17.2.2",
    "openai": "^5.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/debug": "^4.1.12",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.4.0",
    "jest": "^30.2.0",
    "jiti": "^2.6.0",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.45.0"
  }
}
